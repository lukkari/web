article.schedule(class!='<%= data.extraClass %>')
  div.title: h2#scheduleTitle <%= data.title %>
  div.switcher: a(href!='/<%= data.switcher %>' data-local) Switch
  div#calendar.clearfix
  div#weekBar
  div#editBar.schedule-edit-bar
    div#editInit
      button#edit
    div#editProgress
      button#done
      button#add
  div#days
  div#selectSubject
