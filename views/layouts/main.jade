doctype html
html
  head
    title= title
    meta(charset='utf-8')
    link(rel='stylesheet', href='/stylesheets/template.css')
    if mobile
      meta(name="viewport" content="width=device-width, user-scalable=no")
      link(rel='stylesheet', href='/stylesheets/mobile.css')
    block customHead
  body
    div.globalouter
      block content

      div.footer
        span.footlinks
          block footlinks
        span by
          a(href='https://github.com/zaynetro' target='_blank') zaynetro
          | 2014

    block customFoot